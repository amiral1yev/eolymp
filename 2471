# Чтение входных данных
n = int(input())  # Количество холмов

# Чтение матрицы смежности
for i in range(1, n + 1):
    row = list(map(int, input().split()))  # Матрица смежности в строке
    for j in range(i + 1, n + 1):  # Проверка только верхней части матрицы (i < j)
        if row[j - 1] == 1:  # Есть мост
            print(i, j)
